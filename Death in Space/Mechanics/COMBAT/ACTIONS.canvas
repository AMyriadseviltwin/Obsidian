		{"id":"a95b53c742c531b3","type":"text","text":"Run","x":450,"y":-436,"width":103,"height":65},
		{"id":"082a7380375f26d2","type":"text","text":"Improvise","x":1960,"y":-433,"width":160,"height":60},
		{"id":"e735ad93977f3ce4","type":"text","text":"Use Ability","x":1560,"y":-433,"width":180,"height":60},
